<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>switches</title>
  <%- include('../../imports'); -%>
</head>
<body>
  <%- include('../../navbar'); -%>
  <div class="container">
    <div class="row">
        <h1>Administração de switches</h1>
    </div>

    <div class="row mb-2">
        <a href="/admin/switches/create" class="btn btn-success">Novo switch</a>
    </div>

    <div class="row">
        <table class="table">
            <thead class="thead-dark">
              <tr>
                <th scope="col">Código</th>
                <th scope="col">Título</th>
                <th scope="col">Descrição</th>
                <th scope="col">Imagem</th>
              </tr>
            </thead>
            <tbody>
              <% switches.forEach((controlador) => { %>
                <tr>
                  <td><%= controlador.id %></td>
                  <td><%= controlador.title %></td>
                  <td><%= controlador.description %></td>
                  <td><%= controlador.image %></td>
                </tr>
              <% }); %>
              
            </tbody>
          </table>
    </div>
  </div>
</body>
</html>